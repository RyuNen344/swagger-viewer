{
  "name": "swagger-viewer",
  "version": "2.0.0",
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "scripts": {
    "build": "webextension-toolbox build chrome",
    "dev": "webextension-toolbox dev chrome",
    "eslint-print-config": "eslint --print-config src/index.tsx > misc/eslint-print-config.json",
    "lint-dry": "npm-run-all --parallel --continue-on-error lint:eslint-dry lint:format-dry lint:tsc",
    "lint:eslint-dry": "eslint -c .eslintrc.js --ext .js,.ts,.tsx .",
    "lint:eslint": "eslint -c .eslintrc.js --fix --ext .js,.ts,.tsx .",
    "lint:format-dry": "prettier --check .",
    "lint:format": "prettier --write .",
    "lint:tsc": "tsc --noEmit",
    "lint": "npm-run-all --parallel --continue-on-error lint:eslint lint:format lint:tsc",
    "test-ci": "jest --ci",
    "test": "jest --watch",
    "update-all": "npm-check-updates -u --reject \"/^(@types/node)$/\""
  },
  "devDependencies": {
    "@types/chrome": "0.0.127",
    "@types/jest": "26.0.19",
    "@types/js-yaml": "3.12.5",
    "@types/jsdom": "16.2.5",
    "@types/node": "12.12.6",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-test-renderer": "17.0.0",
    "@types/styled-components": "5.1.7",
    "@types/webpack": "4.41.25",
    "@typescript-eslint/eslint-plugin": "4.11.1",
    "@typescript-eslint/parser": "4.11.1",
    "babel-jest": "26.6.3",
    "babel-preset-react-app": "10.0.0",
    "css-loader": "5.0.1",
    "eslint": "7.16.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-only-error": "1.0.2",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-sort-destructure-keys": "1.3.5",
    "eslint-plugin-sort-keys-fix": "1.1.1",
    "eslint-plugin-standard": "4.1.0",
    "eslint-plugin-typescript-sort-keys": "1.5.0",
    "import-sort-style-module": "6.0.0",
    "jest": "26.6.3",
    "npm-check-updates": "10.2.5",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "prettier-plugin-import-sort": "0.0.6",
    "regenerator-runtime": "0.13.7",
    "style-loader": "2.0.0",
    "terser-webpack-plugin": "4.2.3",
    "typescript": "4.1.3",
    "typescript-styled-plugin": "0.15.0",
    "webextension-toolbox": "3.0.0"
  },
  "dependencies": {
    "buffer": "6.0.3",
    "jest-styled-components": "7.0.3",
    "js-yaml": "3.14.1",
    "jsdom": "16.4.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-test-renderer": "17.0.1",
    "styled-components": "5.2.1",
    "swagger-ui-react": "3.38.0"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
